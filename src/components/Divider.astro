  width={width}
  height={height}
  viewBox="0 0 82 6"
  fill="none"
  xmlns="http://www.w3.org/2000/svg"
>
  <path
    d="M1 5C9.59335 3.8642 25.5115 1 40.0793 1L39.8747 3.51852C53.1739 3.17284 77.5627 2.0963 81 2.92593"
    stroke={color}
    stroke-width="2"></path>
